{
  "bin": [
    "bin/predis-cli"
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": "^5.5.9|>=7.0.8",
    "predis/predis": "^1.1",
    "seld/phar-utils": "^1.0",
    "symfony/console": "^3.4",
    "symfony/finder": "^4.3",
    "symfony/process": "^4.3"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Pifeifei\\PredisCli\\": "src"
    },
    "classmap": [
      "src"
    ]
  },
  "extra":{
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
